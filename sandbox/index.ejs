<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body spellcheck="false">
  <section class="colorbomb">
    <div class="function">
      <form>
        colorbomb(<span class="fields">"<input type="text" name="colorbomb">"</span>)
      </form>
    </div>
    <div class="swatches">
      <div class="swatch hex">
        <span>hex(a)</span>
        <span class="value"></span>
      </div>
      <div class="swatch rgb">
        <span>rgb(a)</span>
        <span class="value"></span>
      </div>
      <div class="swatch hsl">
        <span>hsl(a)</span>
        <span class="value"></span>
      </div>
      <div class="error"></div>
    </div>
  </section>
</body>
</html>
